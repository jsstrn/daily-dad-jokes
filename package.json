{
  "name": "daily-dad-jokes",
  "version": "1.0.0",
  "description": "Broadcast a dad jokes to your Telegram channel",
  "author": "",
  "license": "MIT",
  "scripts": {
    "prune": "sls prune --number 3",
    "prune:dev": "npm run prune -- --stage dev",
